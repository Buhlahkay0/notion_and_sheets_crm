{
    "manifest_version": 3,
    "name": "Email Filler",
    "version": "1.0",
    "description": "Send emails with tracking",
    "permissions": ["activeTab", "scripting", "tabs", "storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "mail_icon.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.outlook.com/*"]
      }
    ]
  }
  